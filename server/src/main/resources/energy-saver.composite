<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://www.osoa.org/xmlns/sca/1.0" 
	xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1" 
	xmlns:frascati-upnp="http://frascati.ow2.org/xmlns/upnp/1.0"
	name="energy-saver">

  <service name="energy-saver" promote="energy-saver-component/energy-saver-service">
  	 <frascati:binding.jsonrpc uri="http://localhost:9002/JSON-RPC/EnergySaver"/>
  </service>

  <component name="energy-saver-component">
  	<implementation.java class="org.ow2.frascati.demonstration.ase.energy.EnergySaverImpl"/>
  	<service name="energy-saver-service">
  		<interface.java interface="org.ow2.frascati.demonstration.ase.energy.EnergySaverService"/>
  	</service>
    <reference name="os-command-service">
      <interface.java interface="org.ow2.frascati.demonstration.ase.os.CommandProcessorService"/>
    </reference>  	
    <reference name="interface-manager">
      <interface.java interface="org.ow2.frascati.demonstration.ase.deployer.InterfaceManagerService"/>
      <frascati:binding.jsonrpc uri="http://192.168.1.5:9003/JSON-RPC/InterfaceManager"/>
    </reference>
  </component>
  
  <component name="os-command-component">  
  	<implementation.java class="org.ow2.frascati.demonstration.ase.os.CommandProcessorImpl"/>    	
  	<service name="os-command-service">
      <interface.java interface="org.ow2.frascati.demonstration.ase.os.CommandProcessorService"/>
    </service>        
  </component>
  
  <wire source="energy-saver-component/os-command-service" target="os-command-component/os-command-service"/>
  
</composite>